#include <stdio.h>

#define SIZE 5 // Tama√±o del arreglo

|void morverizq(int arr[], int size){
    int temp = arr[0];

    for (int i=0; i<size-1; i++){
        arr[i] = arr[i + 1];
    }

    arr[size - 1] = temp; //
}

int main() {
    int array[SIZE] = {1, 2, 3, 4, 5};

    printf("Arreglo original: ");
    for (int i=0; i<SIZE; i++){
        printf("%d ", array[i]);
    }
    printf("\n");

    for(int i = 0; i < 4; i++){
        morverizq(array, SIZE);
    }

    printf("Arreglo desplazado 4 veces a la izquierda: ");
    for (int i=0; i<SIZE; i++) {
        printf("%d ", array[i]);
    }
    printf("\n");

    return 0;
}
